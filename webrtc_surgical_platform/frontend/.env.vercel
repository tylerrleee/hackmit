# Vercel Deployment Environment Variables
# These will be configured in Vercel dashboard as environment variables

# API Backend URL (configured in Vercel dashboard)
# REACT_APP_API_URL=https://your-backend-service.railway.app
# Or: REACT_APP_API_URL=https://your-backend-service.render.com

# WebSocket URLs for real-time communication
# REACT_APP_WS_URL=wss://your-backend-service.railway.app
# REACT_APP_AR_BRIDGE_URL=wss://your-bridge-service.railway.app

# Deployment configuration
REACT_APP_DEPLOYMENT_TARGET=vercel
REACT_APP_ENVIRONMENT=production

# Build optimization
GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true

# PWA Configuration
PUBLIC_URL=/
REACT_APP_PWA_ENABLED=true

# Feature flags for Vercel deployment
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_CACHING=true
REACT_APP_ENABLE_SERVICE_WORKER=true

# CORS and Security
REACT_APP_ENABLE_STRICT_CORS=true
REACT_APP_ALLOWED_ORIGINS=["https://*.vercel.app"]

# Performance settings
REACT_APP_CHUNK_SIZE_LIMIT=244000
REACT_APP_BUNDLE_ANALYZER=false